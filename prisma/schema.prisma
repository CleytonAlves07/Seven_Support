generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Tecnico {
  id         String   @id @default(uuid())
  nome       String
  created_at DateTime @default(now())

  @@map("tecnicos")
}

model Cliente {
  id         String   @id @default(uuid())
  nome       String
  empresa    String
  telefone   String
  created_at DateTime @default(now())
}
